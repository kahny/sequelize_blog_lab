<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body>
	<ul >
        
        <li><a href="/newposts">New Post</a></li>
    </ul>
 
     <% posts.forEach(function(post) { %>
     	<h3><%= post.title %></h3>
     	<p><%= post.content %></p>
     	<p><em><%= post.createdAt %></em></p>
     	<p>Post by: <%= post.authorId %></p>

    <% }) %>

</body>
</html>

